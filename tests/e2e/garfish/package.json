{
  "private": true,
  "name": "@e2e/garfish",
  "version": "2.9.0",
  "scripts": {
    "test": "pnpm run test:webpack && pnpm run test:rspack && pnpm run test:rspack:legacy",
    "test:webpack": "BUNDLER=webpack playwright test --workers 1",
    "test:rspack": "BUNDLER=rspack playwright test --workers 1",
    "test:rspack:legacy": "BUNDLER=rspack USE_RSPACK_LEGACY=true playwright test --workers 1"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@modern-js/e2e": "workspace:*",
    "@modern-js/utils": "workspace:*",
    "@types/node": "^14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@playwright/test": "1.33.0",
    "typescript": "^5"
  }
}
